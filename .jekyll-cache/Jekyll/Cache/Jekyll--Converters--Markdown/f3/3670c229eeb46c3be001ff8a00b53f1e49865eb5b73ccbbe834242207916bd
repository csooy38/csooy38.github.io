	<span class="n">sql</span> <span class="o">=</span> <span class="s">"select * from (select row_number() over(order by board_no desc) rnum, b.* from board b where board_title like ? or board_cont like ? or board_writer like ?) where rnum &gt;= ? and rnum &lt;= ?"</span><span class="o">;</span>
		
