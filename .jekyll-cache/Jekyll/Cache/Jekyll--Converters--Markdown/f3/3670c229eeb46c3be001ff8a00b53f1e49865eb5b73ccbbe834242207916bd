	<span class="n">openConn</span><span class="o">();</span>	<span class="c1">// ì‹±ê¸€í†¤&amp;ì»¤ë„¥íŠ¸í’€ ë°©ì‹ìœ¼ë¡œ DBì— ì—°ê²°</span>
	
	<span class="k">if</span> <span class="o">(</span><span class="n">find_field</span><span class="o">.</span><span class="na">equals</span><span class="o">(</span><span class="s">"all"</span><span class="o">))</span> <span class="o">{</span>
